{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "categorie",
    "pluralName": "categories",
    "displayName": "category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "store_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::store.store",
      "inversedBy": "categories"
    },
    "category_name": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category_id"
    }
  }
}
